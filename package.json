{

    "name": "theme-cn_default",

    "sencha": {
        "namespace": "Ext",
        "type": "theme",
        "extend": "theme-triton",
        "toolkit": "classic",
        "creator": "conjoon Open Source Project",
        "summary": "The default theme for conjoon.",
        "detailedDescription": "This package contains overrides, sass and css directives for theming the conjoon application along with its packages.",
        "version": "1.0.0",
        "compatVersion": "1.0.0",
        "format": "1",
        "slicer": {
            "js": [
                {
                    "path": "${package.dir}/sass/example/custom.js",
                    "isWidgetManifest": true
                }
            ]
        },
        "output": {
            "base" : "${package.dir}/build",
            "js": {
                "version": "ES6"
            }
        },
        "compressor": {
            "polyfills": "none"
        },
        "local": true,
        "theme": "theme-triton",
        "sass" : {
            "namespace": "",
            "etc": [
                "${package.dir}/sass/etc/all.scss"
             ],
            "var": [
                "${package.dir}/sass/var/"
            ],
            "src": [
                "${package.dir}/sass/src/"
            ]
        },
        "classpath": [
            "${package.dir}/src"
        ],
        "overrides": [
            "${package.dir}/overrides"
        ],
        "example": null,
        "framework": "ext",
        "requires": [
            "lib-cn_navport"
        ]
    }
}
